[remote "iina"]
    url = git@github.com:iina/iina.git
    fetch = +refs/heads/*:refs/remotes/iina/*
[remote "origin"]
    url = git@github.com:huangyingw/iina_iina.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = iina
    target = iina/develop
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
    ignore = *.sh
[deploy]
	host = localhost
	path = ~/myproject/git/tools/iina/iina
